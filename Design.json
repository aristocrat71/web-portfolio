{
  "name": "MitulShethPortfolioLayout",
  "description": "Masonry grid layout portfolio using React and reactbits.dev Masonry. Includes contact links, profile intro, location, experience, and project sections.",
  "tech": ["React", "JavaScript", "TailwindCSS", "reactbits.dev Masonry"],
  "components": [
    {
      "name": "MasonryGrid",
      "type": "layout",
      "library": "reactbits",
      "props": {
        "columns": {
          "default": 4,
          "responsive": {
            "768px": 2,
            "1024px": 3
          }
        },
        "gap": "1rem"
      },
      "children": [
        { "component": "TileIntro" },
        { "component": "TileLocation" },
        { "component": "TileDeveloper" },
        { "component": "TileAbout" },
        { "component": "TileExperience" },
        { "component": "TileProjects" },
        { "component": "TileConnect" },
        { "component": "TileGithub" },
        { "component": "TileLinkedIn" }
      ]
    },
    {
      "name": "TileIntro",
      "type": "card",
      "props": {
        "text": "Hi There! I am Mitul Sheth.",
        "size": "medium",
        "bg": "white",
        "rounded": "xl"
      }
    },
    {
      "name": "TileLocation",
      "type": "card",
      "props": {
        "icon": "map-pin",
        "text": "Pune, India",
        "size": "small",
        "bg": "white",
        "rounded": "xl"
      }
    },
    {
      "name": "TileDeveloper",
      "type": "card",
      "props": {
        "text": "Software Developer\n//Data Science",
        "size": "medium",
        "bg": "white",
        "rounded": "xl"
      }
    },
    {
      "name": "TileAbout",
      "type": "button",
      "props": {
        "text": "About me",
        "onClick": "navigateTo('about')",
        "size": "small",
        "bg": "white",
        "rounded": "xl"
      }
    },
    {
      "name": "TileExperience",
      "type": "button",
      "props": {
        "text": "Experience",
        "onClick": "navigateTo('experience')",
        "size": "small",
        "bg": "white",
        "rounded": "xl"
      }
    },
    {
      "name": "TileProjects",
      "type": "button",
      "props": {
        "text": "Projects",
        "onClick": "navigateTo('projects')",
        "size": "medium",
        "bg": "white",
        "rounded": "xl"
      }
    },
    {
      "name": "TileConnect",
      "type": "button",
      "props": {
        "text": "Let's Connect",
        "onClick": "navigateTo('contact')",
        "orientation": "vertical",
        "size": "large",
        "bg": "white",
        "rounded": "xl"
      }
    },
    {
      "name": "TileGithub",
      "type": "iconButton",
      "props": {
        "icon": "github",
        "link": "https://github.com/aristocrat71",
        "bg": "gray-100",
        "rounded": "full"
      }
    },
    {
      "name": "TileLinkedIn",
      "type": "iconButton",
      "props": {
        "icon": "linkedin",
        "link": "https://linkedin.com/in/yourusername",
        "bg": "gray-100",
        "rounded": "full"
      }
    }
  ]
}
